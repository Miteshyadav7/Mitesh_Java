import java.util.*;

class Setmain {
    public static void main(String args[]) {
        Set<Integer> sa = new HashSet<>(Arrays.asList(1, 2, 3, 4));
        Set<Integer> sb = new HashSet<>(Arrays.asList(2, 3, 4));

        if (sa.containsAll(sb)) {

            System.out.println("a is subset of b ");
        } else {
            System.out.println("a is not   subset of b ");
        }

    }
}
