import java.util.Scanner;
class Example{
public static void main(String [] args){
Scanner sc = new Scanner(System.in);
System.out.print("enter a range of no");
int n = sc.nextInt();
int age[] = new int[n];
for(int i=0;i<n;i++)
{
System.out.print("enter a no");
age [i] = sc.nextInt();
}
for(int i=0;i<n;i++)
{
if(age[i]%2==0)
{
System.out.print(age[i]+"is even no ");
}
}
}}
