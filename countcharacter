class Jan15 {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        String os = sc.nextLine();
        char s = sc.next().charAt(0);
        int count = 0;
        for (int i = 0; i < os.length(); i++) {
            if (os.charAt(i) == s) {
                count++;
            }
        }
        System.out.println(count + "character are available");
    }
}
