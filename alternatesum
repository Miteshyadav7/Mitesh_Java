import java.util.Scanner;
class Example{
public static void main(String [] args){
Scanner sc = new Scanner(System.in);
System.out.print("enter a range of no");
int n = sc.nextInt();
int age[] = new int[n];
for(int i=0;i<n;i++)
{
System.out.print("enter a no");
age [i] = sc.nextInt();
}
int sum =0;
for(int i=0;i<n;i=i+2)
{
 sum+=age[i];
}
System.out.print(sum);

}}
