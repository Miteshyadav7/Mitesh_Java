class Example{
public static void main(String [] args){
System.out.println("enter 2 no and 1st no is greater");
Scanner sc = new Scanner(System.in);

System.out.print("a = ");
int a = sc.nextInt();
System.out.print("b = ");     
int b = sc.nextInt();
int c = a/b;
if(a>=0 && a>b)
{
System.out.println(c);
}
else
{
System.out.println("output is invalid");
}
