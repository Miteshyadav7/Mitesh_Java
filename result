class Example{
public static void main(String [] args){
int marks = 99;
if (marks<30)
{
 System.out.println ("fail");
}
else if (marks<60)
{
System.out.println ("E");
}
else if (marks<70)
{
System.out.println ("D");
} 
else if (marks<80)
{
System.out.println ("C");
}
else if (marks<90)
{
System.out.println ("B");
}
else if (marks<100)
{
System.out.println ("A");
}       
}
}
